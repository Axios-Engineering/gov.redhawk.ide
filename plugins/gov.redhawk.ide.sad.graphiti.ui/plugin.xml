<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypeProviders">
      <diagramTypeProvider
            class="gov.redhawk.ide.sad.graphiti.ui.diagram.providers.SADDiagramTypeProvider"
            id="gov.redhawk.ide.sad.graphiti.ui.FactoryProvider"
            name="FactoryProvider">
         <diagramType
               id="gov.redhawk.ide.sad.graphiti.ui.SADDiagramEditor">
         </diagramType>
         <imageProvider
               id="gov.redhawk.ide.sad.graphiti.ui.diagram.providers.imageProvider">
         </imageProvider>
      </diagramTypeProvider>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.diagramTypes">
      <diagramType
            id="gov.redhawk.ide.sad.graphiti.ui.SADDiagramEditor"
            name="SAD Diagram"
            type="SADDiagram">
      </diagramType>
   </extension>
   <extension
         point="org.eclipse.graphiti.ui.imageProviders">
      <imageProvider
            class="gov.redhawk.ide.sad.graphiti.ui.diagram.providers.ImageProvider"
            id="gov.redhawk.ide.sad.graphiti.ui.diagram.providers.imageProvider">
      </imageProvider>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated RHGraphiti -->
      <package
            uri="http://www.redhawk.gov/model/rhgext/1.0.0"
            class="gov.redhawk.ide.sad.graphiti.ext.RHGxPackage"
            genModel="model/RHGraphiti.xcore"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.content_parser">
      <!-- @generated RHGraphiti -->
      <parser
            contentTypeIdentifier="http://www.redhawk.gov/model/rhgext/1.0.0"
            class="org.eclipse.emf.ecore.xmi.impl.XMIResourceFactoryImpl"/>
   </extension>

   <extension point="org.eclipse.core.contenttype.contentTypes">
      <!-- @generated RHGraphiti -->
      <content-type
            base-type="org.eclipse.emf.ecore.xmi"
            file-extensions="rhgx"
            id="http://www.redhawk.gov/model/rhgext/1.0.0"
            name="%_UI_RHGx_content_type"
            priority="normal">
         <describer class="org.eclipse.emf.ecore.xmi.impl.RootXMLContentHandlerImpl$Describer">
            <parameter name="namespace" value="http://www.redhawk.gov/model/rhgext/1.0.0"/>
            <parameter name="kind" value="xmi"/>
         </describer>
      </content-type>
   </extension>
   
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="SADDiagram.PropertyContributor">
         <propertyCategory
               category="Graphiti">
         </propertyCategory>
      </propertyContributor>
   </extension>
   
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="SADDiagram.PropertyContributor">

        <propertyTab
                category="Graphiti"
                id="property.tab.advanced"
                label="Advanced">
        </propertyTab>
           
         <propertyTab
             category="Graphiti"
             id="property.tab.ComponentProperties"
             label="Component Properties"/>

     </propertyTabs>
   </extension>

   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections contributorId="SADDiagram.PropertyContributor">
      
         
         <propertySection
         	   class="gov.redhawk.ide.sad.graphiti.ui.properties.ComponentPropertiesSection"
               filter="gov.redhawk.ide.sad.graphiti.ui.properties.ComponentFilter"
               id="property.section.ComponentProperties"
               tab="property.tab.ComponentProperties">
         </propertySection>


         <propertySection
         	class="org.eclipse.ui.views.properties.tabbed.AdvancedPropertySection"
         	filter="gov.redhawk.ide.sad.graphiti.ui.properties.ComponentFilter"
         	id="graphiti.main.tab.emfreference.tableddd"
         	tab="property.tab.advanced">
         </propertySection>

         
      </propertySections>
   </extension>
   
      
   <!--
        Registers an Eclipse adapter factory to provide IPropertySource objects for the GraphitiShapeEditPart
        that gets selected in the DiagramEditor to provide a standard (table format) property sheet for EReference
        objects.
        -->
        <extension
       point="org.eclipse.core.runtime.adapters">
                <factory
                        adaptableType="org.eclipse.graphiti.ui.platform.GraphitiShapeEditPart"
                        class="gov.redhawk.ide.sad.graphiti.ui.properties.GraphitiEditPartToIPropertySourceAdapterFactory">
                        <adapter
                                type="org.eclipse.ui.views.properties.IPropertySource">
                        </adapter>
                </factory>
        </extension>
</plugin>
